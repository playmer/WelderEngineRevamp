[Vertex]
struct BasicVertexFrag
{
  [StageInput]
  var LocalPosition : Real3;
  [StageInput]
  var Uv : Real2;
  [StageInput]
  var Color : Real4;
  
  
  [HardwareBuiltInOutput]
  var ApiPerspectivePosition : Real4;
  
  function Main()
  {
    this.ApiPerspectivePosition = Real4(this.LocalPosition, 1);
  }
}
